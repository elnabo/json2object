-cp src
-main tests.Main
-lib hxjsonast
-lib utest
-D travis # for utest report
--macro include("json2object")
--each

# Js
-cmd echo "---- Js -----"
-js testout/test.js
-cmd node testout/test.js
--next

# Lua
-cmd echo "---- Lua -----"
-lua testout/test.lua
# -cmd lua testout/test.lua
--next

# Flash
-cmd echo "---- Flash -----"
-swf testout/test.swf
#-cmd
--next

# Neko
-cmd echo "---- Neko -----"
-neko testout/test.n
-cmd neko testout/test.n
--next

# Php
-cmd echo "---- Php -----"
-php testout/php
-D php7
-cmd php testout/php/index.php
--next

# Cpp
-cmd echo "---- Cpp -----"
-debug
-cpp testout/cpp/
-cmd ./testout/cpp/Main-debug
--next

# Cppia
-cmd echo "---- Cppia -----"
-debug
-cppia testout/test.cppia
# -cmd haxelib run hxcpp testout/test.cppia
--next

# C#
-cmd echo "---- Cs -----"
-cs testout/cs/
-cmd mono ./testout/cs/bin/Main.exe
--next

# Java
-cmd echo "---- Java -----"
-java testout/java/
-cmd java -jar testout/java/Main.jar
--next

# Python
-cmd echo "---- Python -----"
-python testout/test.py
-cmd python3 testout/test.py
--next

# Hl
-cmd echo "---- Hl -----"
-hl testout/test.hl
# -cmd

-cmd echo "done"
